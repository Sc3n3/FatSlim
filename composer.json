{
    "name": "Sc3n3/FatSlim",
    "description": "Slim Framework 2.6 with Twig Template Engine & Eloquent ORM",
    "require": {
        "slim/slim": "2.6.2",
        "slim/extras": "2.0.3",
        "slim/views": "0.1.3",
        "twig/twig": "1.26.1",
        "illuminate/database": "5.3.16",
        "predis/predis": "1.1.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Sc3n3\\FatSlim\\": "app/Libraries"
        },
        "files": [
            "app/Libraries/bootstrap.php",
            "app/Libraries/Cache/Cache.php"
        ]
    }
}
